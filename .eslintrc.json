{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
      "no-param-reassign": "off",
      "camelcase": "off",
      "no-unused-vars": ["error", {"argsIgnorePattern": "next"}],
      "max-len": ["error", {"code": 80}]
    }
}
